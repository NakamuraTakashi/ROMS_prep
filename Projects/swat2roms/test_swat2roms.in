!  RIVER_FILE  = "../../Data/todoroki/channel_day.txt"   ! Todoroki river

!  cha_name = "cha002" ! channel name of the river mouth.
  cha_name = "cha02" ! channel name of the river mouth.
!  cha_name = "1" ! channel name of the river mouth.

! If the SWAT+ output of water quarity parameters (sed, no3, etc.) are yield (not concentration),
! Turn on the flag. (.TRUE. / .FALSE.)
  Yield = .TRUE. 

  LOCAL_TIME = 9 ! UTC+9; Set time-zone difference between local time and GMT in the input data.

! --- Input data setting --------------------
!
! *_Label: label of the target values. 
!          If set "Null", a constant value that sets at "*0" is applied.
! *_sf:  scale_factor
! *_off: offset
! output_value = input_value * scale_factor + offset

! Flow
  Flow0 = 1.0d0 ! m3/s
  Flow_Label = "flo_out"
!  Flow_Label = "flow_m^3/s" 
  Flow_sf  = 1.0d0   ! m3/s -> m3/s
  Flow_off = 0.0d0

! temp
  T0 = 25.0d0  ! deg.C
  T_Label = "Null" 
!  T_Label = "temp_out" 
!  T_Label = "temp" 
  T_sf  = 1.0d0
  T_off = 0.0d0

! salt
  S0 = 1.0d0   ! psu
  S_Label = "Null" 
  S_sf  = 1.0d0
  S_off = 0.0d0

! mud_
  MUD0 = 1.0d0, 1.0d0    ! mud_  (kg m-3)
  MUD_Label = "sil_out", "cla_out" 
!  MUD_Label = "sed_tons", "sed_tons" 
!  MUD_Label = "Null", "Null" 
  MUD_sf  = 1.16d-3, 1.16d-3  ! tons/day ->100/(24*60*60) = 1.16*10^-3 kg/s
  MUD_off = 0.0d0, 0.0d0

! sand_
  SAND0 = 1.0d0    ! sand_  (kg m-3)
  SAND_Label = "san_out"
!  SAND_Label = "sed_tons"
!  SAND_Label = "Null"
  SAND_sf  = 1.16d-3  ! tons/day ->100/(24*60*60) = 1.16*10^-3 kg/s
  SAND_off = 0.0d0

! TIC
